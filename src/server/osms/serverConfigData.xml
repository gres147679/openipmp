<osms:ServerConfigurationData xmlns:osms="http://www.openipmp.org/">
	<ServerConstants>
		<Server>
			<Port>8080</Port>
		</Server>
		<Database>
			<Provider>MYSQL</Provider>
			<Driver>com.mysql.jdbc.Driver</Driver>
			<ConnectURL>jdbc:mysql://localhost:3306/OSMS?zeroDateTimeBehavior=convertToNull</ConnectURL>
			<UserName>osms</UserName>
			<Password>openipmp</Password>
		</Database>
		<Logs>
			<Debug>
				<FileName>debug.log</FileName>
				<Path>Logs</Path>
			</Debug>
			<Info>
				<FileName>info.log</FileName>
				<Path>Logs</Path>
			</Info>
			<Error>
				<FileName>error.log</FileName>
				<Path>Logs</Path>
			</Error>
			<All>
				<FileName>all.log</FileName>
				<Path>Logs</Path>
			</All>
		</Logs>
		<Security>
			<SymmetricEncryption>
				<Algorithm>DES</Algorithm>
				<Provider>ECB</Provider>
				<Padding>PKCS5Padding</Padding>
				<KeySize>128</KeySize>
			</SymmetricEncryption>
			<AsymmetricEncryption>
				<Algorithm>RSA</Algorithm>
				<Provider>BC</Provider>
				<KeySize>768</KeySize>
			</AsymmetricEncryption>
			<MasterKeys>
				<MasterKey>
					<KeyId>1</KeyId>
					<FileName>mastersecret.ser</FileName>
					<Path></Path>
				</MasterKey>
				<MasterKey>
					<KeyId>2</KeyId>
					<FileName>mastersecret2.ser</FileName>
					<Path></Path>
				</MasterKey>
			</MasterKeys>
		</Security>
	</ServerConstants>
	<ApplicationConstants>
		<DOI>
			<Code>DOI</Code>
			<ApplicationProfile>KERNEL</ApplicationProfile>
			<Master>
				<Prefix>10.9901/12.m.</Prefix>
				<SuffixTable>doi_master_sequence</SuffixTable>
			</Master>
			<Instance>
				<Prefix>10.9901/12.i.</Prefix>
				<SuffixTable>doi_instance_sequence</SuffixTable>
			</Instance>
			<Assertor>
				<Authority>
					<Name>Mutable, Inc</Name>
					<Type>Organization</Type>
					<Identifier>10.9901/1</Identifier>
					<URL>www.mutable.com</URL>
				</Authority>
				<Registrant>
					<OrganizationType>Organization</OrganizationType>
					<PersonType>Person</PersonType>
				</Registrant>
			</Assertor>
		</DOI>
		<GRID>
			<Code>GRID</Code>
		</GRID>
		<Templates>
			<DOI>
				<FileName>Template_DOI.xml</FileName>
				<Path>Templates</Path>
			</DOI>
			<AuxData>
				<FileName>Template_AuxData.xml</FileName>
				<Path>Templates</Path>
			</AuxData>
			<Signature>
				<FileName>Template_Signature.xml</FileName>
				<Path>Templates</Path>
			</Signature>
			<LicenseODRL>
				<FileName>Template_LicenseODRL.xml</FileName>
				<Path>Templates</Path>
			</LicenseODRL>
                  <EncryptedKey>
                        <FileName>Template_EncryptedKey.xml</FileName>
                        <Path>Templates</Path>
                  </EncryptedKey>
		</Templates>
		<RightsHost>
			<URL>localhost</URL>
			<Port>8080</Port>
		</RightsHost>
		<Intent>
			<Play>PLAY</Play>
		</Intent>
		<Audit>
			<CreateUser>TestCreateUser</CreateUser>
			<UpdateUser>TestUpdateUser</UpdateUser>
		</Audit>

		<Encoder>
			<Path>/usr/bin/mp4creator</Path>
		</Encoder>
		<ExternalAuth>
		<SPEAuthUser>false</SPEAuthUser>
	</ExternalAuth>

	</ApplicationConstants>
</osms:ServerConfigurationData>
